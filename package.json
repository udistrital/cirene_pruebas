    "gulp-rm": "^1.0.4",
    "gulp-tar": "^2.0.0",
    "gulp-webserver": "^0.9.1",
    "jasmine-core": "^2.7.0",
    "jsdoc": "^3.5.4",
    "jshint": "^2.9.5",
